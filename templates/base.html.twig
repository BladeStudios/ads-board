<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        <link type="text/css" rel="stylesheet" href="{{ asset('../assets/css/bootstrap-5.0.2.css') }}">
        <link type="text/css" rel="stylesheet" href="{{ asset('../assets/css/style.css') }}">
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('../assets/js/jquery-3.6.1.min.js') }}"></script>
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% block body %}
<header class="line">
    <nav class="navbar navbar-expand-lg-modified navbar-light bg-light">
    <div class="container-fluid width-80">
    <span class="logo bg-light"><a class="a-no-css" href="/">AdsBoard</a></span>
    <div id="navbarSupportedContent">
        <ul class="navbar-nav ms-auto mb-lg-0">
            <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="{{ path('index') }}">Strona główna</a>
            </li>
            <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Dodaj ogłoszenie</a>
            </li>
            <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Szukaj ogłoszeń</a>
            </li>
            <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">API</a>
            </li>
        </ul>
    </div>
    </div>
    </nav>
</header>
{% endblock %}
</body>
</html>
